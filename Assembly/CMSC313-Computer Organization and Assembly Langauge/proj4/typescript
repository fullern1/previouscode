[1m[34mbookcmp[0m[39;49m:[K
        [1m[36mpush[0m[39;49m ebx                [31m; save registers used just in case              [39;49m
[A
        [1m[36mpush[0m[39;49m ecx[K
        [1m[36mpush[0m[39;49m edx[K
        [1m[36mpush[0m[39;49m edi[K
[K
        [1m[36mmov[0m[39;49m ebx, [book1]        [31m; book1 pointer                                 [39;49m
[A
        [1m[36mmov[0m[39;49m ecx, [book2]        [31m; book2 pointer                                 [39;49m
[A
        [1m[36mmov[0m[39;49m edi, 0              [31m; counter to increment                          [39;49m
[A
        [1m[36mmov[0m[39;49m eax, 0[K
        [1m[36mmov[0m[39;49m edx, 0[K
        [1m[36mjmp[0m[39;49m .nameLoop[K
        [1m[36mmov[0m[39;49m eax, [ebx+YEAR_OFFSET] [31m; move book1 year to eax                     [39;49m
[A
        [1m[36mmov[0m[39;49m edx, [ecx+YEAR_OFFSET] [31m; move book2 year to edx                     [39;49m
[A
[K
        [1m[36mcmp[0m[39;49m eax, edx            [31m; compare to see which is bigger                [39;49m
[A
        [1m[36mjg[0m[39;49m .greater             [31m; if greater jump to .greater and return 1      [39;49m
[A
        [1m[36mjl[0m[39;49m .less                [31m; if less jump to .less and return -1           [39;49m
[A
        [1m[36mjmp[0m[39;49m .cmpName            [31m; book years are equal, cmp name                [39;49m
[A
[K
[1m[34m.cmpName[0m[39;49m:[K
        [1m[36mmov[0m[39;49m edi, 0              [31m; reset counter to 0                            [39;49m
[A
[1m[34m.nameLoop[0m[39;49m:[K
        [1m[36mmov[0m[39;49m ah, [ebx+edi+TITLE_OFFSET][K
        [1m[36mmov[0m[39;49m dh, [ecx+edi+TITLE_OFFSET][K
[K
        [1m[36mcmp[0m[39;49m ah, dh              [31m; check if equal                                [39;49m
[A
        [1m[36mje[0m[39;49m .nullNameCheck       [31m; if so check if null                           [39;49m
[A
[K
        [1m[36mcmp[0m[39;49m ah, 0H              [31m; Check if reached end of book1 title           [39;49m
[A
        [1m[36mje[0m[39;49m .less                [31m; if so book 1 is less                          [39;49m
[A
        [1m[36mcmp[0m[39;49m dh, 0H              [31m; Check if reached end of book2 title           [39;49m
[A
